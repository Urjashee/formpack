<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Fields for versions list index out of range</h:title>
    <model odk:xforms-version="1.0.0">
      <instance>
        <data id="fields_for_versions_list_index_out_of_range" version="ffvlioor_v1">
          <first_but_not_one/>
          <one/>
          <third/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/first_but_not_one" type="string"/>
      <bind nodeset="/data/one" type="string"/>
      <bind nodeset="/data/third" type="string"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <input ref="/data/first_but_not_one">
      <label>first but not one</label>
    </input>
    <input ref="/data/one">
      <label>one</label>
    </input>
    <input ref="/data/third">
      <label>third</label>
    </input>
  </h:body>
</h:html>
