<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:ev="http://www.w3.org/2001/xml-events" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:jr="http://openrosa.org/javarosa" xmlns:odk="http://www.opendatakit.org/xforms" xmlns:orx="http://openrosa.org/xforms" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <h:head>
    <h:title>Field Position with Multiple Versions</h:title>
    <model odk:xforms-version="1.0.0">
      <instance>
        <data id="field_position_with_multiple_versions" version="fpmv_3">
          <group_wy15q80 jr:template="">
            <Firstname/>
            <Lastname/>
            <Gender/>
            <Age/>
          </group_wy15q80>
          <group_wy15q80>
            <Firstname/>
            <Lastname/>
            <Gender/>
            <Age/>
          </group_wy15q80>
          <City/>
          <meta>
            <instanceID/>
          </meta>
        </data>
      </instance>
      <bind nodeset="/data/group_wy15q80/Firstname" required="false()" type="string"/>
      <bind nodeset="/data/group_wy15q80/Lastname" required="false()" type="string"/>
      <bind nodeset="/data/group_wy15q80/Gender" required="false()" type="string"/>
      <bind nodeset="/data/group_wy15q80/Age" required="false()" type="date"/>
      <bind nodeset="/data/City" required="false()" type="string"/>
      <bind jr:preload="uid" nodeset="/data/meta/instanceID" readonly="true()" type="string"/>
    </model>
  </h:head>
  <h:body>
    <group ref="/data/group_wy15q80">
      <label>Person</label>
      <repeat nodeset="/data/group_wy15q80">
        <input ref="/data/group_wy15q80/Firstname">
          <label>Firstname</label>
        </input>
        <input ref="/data/group_wy15q80/Lastname">
          <label>Lastname</label>
        </input>
        <select1 ref="/data/group_wy15q80/Gender">
          <label>Gender</label>
          <item>
            <label>Male</label>
            <value>male</value>
          </item>
          <item>
            <label>Female</label>
            <value>female</value>
          </item>
          <item>
            <label>Other</label>
            <value>other</value>
          </item>
        </select1>
        <input ref="/data/group_wy15q80/Age">
          <label>Age</label>
        </input>
      </repeat>
    </group>
    <input ref="/data/City">
      <label>City</label>
    </input>
  </h:body>
</h:html>
